{
  "paths": {
    "/scores/{experience}": {
      "post": {
        "tags": [
          "score"
        ],
        "description": "Get a set of scores",
        "operationId": "getBulkScore",
        "parameters": [
          {
            "name": "experience",
            "in": "path",
            "description": "The experience to get scores from",
            "required": true,
            "schema": {
              "$ref": "#/components/schemas/Experiences"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/components/schemas/PlayerRequest"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/ScoreResponse"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/score/{experience}": {
      "post": {
        "tags": [
          "score"
        ],
        "description": "Get a specific score",
        "operationId": "getScore",
        "parameters": [
          {
            "name": "experience",
            "in": "path",
            "description": "The experience to get scores from",
            "required": true,
            "schema": {
              "$ref": "#/components/schemas/Experiences"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/PlayerRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ScoreResponse"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "ScoreResponse": {
        "type": "object",
        "properties": {
          "score": {
            "type": "array",
            "items": {
              "type": "number"
            }
          }
        }
      }
    }
  }
}